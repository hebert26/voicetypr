{
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "extraArgs": ["--target-dir", "target/check"]
        },
        "lens": {
          "enable": false
        },
        "inlayHints": {
          "enable": false
        },
        "procMacro": {
          "attributes": {
            "enable": false
          }
        },
        "hover": {
          "actions": {
            "enable": false
          }
        }
      }
    }
  }
}
